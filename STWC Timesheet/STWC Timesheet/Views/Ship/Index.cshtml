@model IEnumerable<STWC_Timesheet.ship>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ship_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ship_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ship_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.ship_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ship_id })
        </td>
    </tr>
}

</table>
